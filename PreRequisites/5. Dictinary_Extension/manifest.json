{
  "manifest_version": 2,

  "name":"Wiki Dictonary",
  "version":"1.0",


  "description": "Find Meaning of words on the go!",


  "icons":{
    "128":"icon_128.png",
    "48" :"icon_48.png",
    "16":"icon_16.png"
  },


  "background":{
    "scripts":["eventPage.js"],
    "persistent":false
  },

  "permissions":[
    "tabs",
    "contextMenus"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net; object-src 'self'"

}
